<?xml version="1.0" encoding="UTF-8"?>  
<beans xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
        http://www.springframework.org/schema/beans/spring-beans.xsd  
        http://code.alibabatech.com/schema/dubbo  
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">  

	<dubbo:application name="user_center_sdk_consumer" />
	
	<dubbo:registry protocol="zookeeper" address="dev-zk-1.tae-tea.net:2181"/>

    <!-- 要引用的服务 -->  
    <dubbo:reference id="userCenterRealNameAuthenticationService" interface="com.yestae.user.center.dubbo.service.IUserCenterRealNameAuthenticationService" check="false" version="1.0"/>
    <dubbo:reference id="userCenterService" interface="com.yestae.user.center.dubbo.service.IUserCenterService" check="false" version="1.0"/>
    <dubbo:reference id="userCenterAddedService" interface="com.yestae.user.center.dubbo.service.IUserCenterAddedService" check="false" version="1.0" timeout="600000"/>
    <dubbo:reference id="smsDubboService" interface="com.yestae.sms.api.dubbo.ISmsDubboService" check="false" version="1.0"/>  
    <dubbo:reference id="userCenterInvoiceService" interface="com.yestae.user.center.dubbo.service.IUserCenterInvoiceService" check="false" version="1.0"/>
    <!--授权委托书下载服务-->
    <dubbo:reference interface="com.yestae.user.tmpl.dubbo.service.IPoaTmplDubboService" id="poaTmplDubboService" check="false" version="1.0" />

</beans>
